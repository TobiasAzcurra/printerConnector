{
  "name": "ticket-connector",
  "version": "1.0.0",
  "description": "Conector para impresión de tickets en impresoras térmicas",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "web": "node web/server.js",
    "dev": "concurrently \"npm run start\" \"npm run web\"",
    "pkg": "pkg .",
    "build:server": "pkg . --targets node16-win-x64 --out-path dist/server",
    "install-service": "node installer-service.js",
    "uninstall-service": "node uninstall-service.js"
  },
  "bin": "index.js",
  "pkg": {
    "assets": [
      "web/**/*",
      "src/**/*",
      "templates/**/*"
    ],
    "scripts": [
      "installer-service.js"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.2",
    "mustache": "^4.2.0",
    "node-thermal-printer": "^4.4.1",
    "node-windows": "^1.0.0-beta.8",
    "opentype.js": "^1.3.4",
    "sharp": "^0.34.1",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "pkg": "^5.8.1"
  }
}
